---
import ForceGraph from "./ForceGraph";

interface Props {
  height?: number;
  width?: number;
  padding?: number;
}

const { height, width, padding } = Astro.props;

const graph = await import("../pages/graph.json");
const graphData = await graph.GET({}).then((response) => response.json());
---

<ForceGraph
  data={graphData}
  height={height}
  width={width}
  padding={padding}
  client:only="react"
/>
